{
  "name": "Remote Request Sender Extension",
  "version": "1",
  "manifest_version": 2,
  "description": "Demonstrate http ajax requests from extension.",
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.1.0.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ]
}